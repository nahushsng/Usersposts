<%= simple_form_for(@app_setting) do |f| %>
  <div>
    <%= f.label :tab_name, style: "display: block" %>
    <%= f.text_field :tab_name %>
  </div>

  <div>
    <%= f.label :theme_name, style: "display: block" %>
    <%= f.input :theme_name, collection: AppSetting::APP_THEME, include_blank: "Select Theme " %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
